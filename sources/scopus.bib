@ARTICLE{de Landa2025139,
	author = {de Landa, Joseba Fernandez and Agerri, Rodrigo},
	title = {Language Independent Stance Detection: Social Interaction-based Embeddings and Large Language Models; [Detección de Stance Independiente del Idioma: Representaciones Vectoriales basadas en Interacciones Sociales y Grandes Modelos de Lenguaje]},
	year = {2025},
