@ARTICLE{Wang2025,
	author = {Wang, Chao and Yang, Jianming and He, Qing},
	title = {Event extraction for visual: De-biasing with causality-guided attention mechanism},
	year = {2025},
	journal = {Neurocomputing},
	volume = {649},
	doi = {10.1016/j.neucom.2025.130783},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-105009477627&doi=10.1016%2fj.neucom.2025.130783&partnerID=40&md5=6f874bcb6de8f59663170dc7f10ae377},
	affiliations = {School of Future Technology, Shanghai University, Shanghai, 200444, China; Institute of Artificial Intelligence, Shanghai University, Shanghai, 200444, China},
	abstract = {The Transformer model has demonstrated remarkable performance in artificial intelligence, achieving unprecedented success across a wide range of tasks. At its core, the attention mechanism remains an indispensable component of advanced Transformer architectures, with its significance and contributions widely acknowledged by the AI community. However, as research deepens and the demand for solving more complex real-world tasks increases, researchers have gradually identified bottlenecks in the self-attention mechanism, with some hypotheses validated through experimental observations. Despite these advances, current studies on the limitations of the self-attention mechanism are primarily centered on empirical observations and experimental results, lacking rigorous theoretical foundations. This gap has led to limited interpretability and unclear rationales behind some findings, despite their potential significance. In this paper, we investigate the limitations of the attention mechanism in Transformer models and provide an in-depth analysis of its capability bottleneck within the data-driven research paradigm. We identify a distinct capability boundary of the traditional attention mechanism and present a comprehensive theoretical analysis based on causal sampling theory. To validate our theoretical findings, we evaluate them in the context of the challenging task of Video Situation Recognition (VSR), obtaining experimental results that align with our theoretical conclusions. We hope that the findings of this study will inspire new perspectives on the capabilities of attention mechanisms and contribute positively to the development of optimization strategies in attention mechanism design. © 2025 Elsevier B.V.},
	author_keywords = {Attention mechanism; Causal inference; Event extraction; Video content parsing},
