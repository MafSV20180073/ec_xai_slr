@ARTICLE{Rama201813,
	author = {Rama, José and Cordero, Guillermo},
	title = {Who are the losers of the economic crisis? Explaining the vote for right-wing populist parties in Europe after the Great Recession; [¿Quiénes son los perdedores de la crisis económica? Hacia una explicación del voto a partidos populistas de derecha en Europa tras la Gran Recesión]},
	year = {2018},
