	title = {Interventional and Counterfactual Causal Reasoning for LLM-based AI Agents: A Dataset and Evaluation in Portuguese; [Razonamiento Causal Intervencional y Contrafactual para Agentes de IA Basados en LLM: Un Conjunto de Datos y Evaluación en Portugués]},
	year = {2025},
	journal = {Procesamiento del Lenguaje Natural},
	volume = {74},
