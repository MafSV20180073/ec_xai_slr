  month={},}@ARTICLE{10460131,
  author={Li, Ye and Yu, Le and Xing, Lu and Liu, Fei},
  journal={IEEE Transactions on Intelligent Vehicles}, 
  title={Analysis of influencing factors of lane change prediction with data missing}, 
  year={2024},
  volume={},
  number={},
  pages={1-13},
  abstract={The accurate behavior prediction of autonomous vehicles relied heavily on precise data supplied by various sensors. In spite of this, data missing due to a communication barrier or equipment failure is an unavoidable issue in the process of data acquisition, which significantly impacts lane-changing (LC) prediction. To solve this issue, the influence of data missing on lane change prediction was explored from the several aspects such as missing pattern, missing proportion and data interpolation. The different degrees of data missing in three patterns, namely block missing, temporally correlated missing and random missing, were examined. Missing values are repaired and LC behavior is predicted based on machine/deep learning models, and hyperparameters are optimized using genetic algorithms. By analyzing the factors that affect the model's generalization ability, the difference between the best data repair result and the best prediction result was further explored. The results show that: (1) the proposed methods can effectively solve the problems of data missing repair and prediction accuracy, especially the KNNImputer for repair and the Transformer for prediction; (2) temporally correlated missing is inferior to random missing for filling in errors, but superior for prediction performances. This indicates that the selection of repair methods should take into account not only the best data imputation, but also the best prediction performance; and (3) there are significant differences in the stability of prediction models under different data missing patterns and the CNN-BiLSTM-SA has achieved relatively good performance},
  keywords={Predictive models;Data models;Behavioral sciences;Trajectory;Sensors;Prediction algorithms;Deep learning;Lane change;Autonomous vehicle;Data missing;Deep learning},
  doi={10.1109/TIV.2024.3372998},
  ISSN={2379-8904},
  month={},}@INPROCEEDINGS{10862202,
  author={Anuprathibha, T. and Praveen, RVS and H, Jayanth and Sukumar, P and Suganthi, G. and Ravichandran, T},
  booktitle={2024 Global Conference on Communications and Information Technologies (GCCIT)}, 
  title={Enhancing Fake Review Detection: A Hierarchical Graph Attention Network Approach Using Text and Ratings}, 
  year={2024},
  volume={},
  number={},
  pages={1-5},
  abstract={The establishment of trusting connections between buyers and sellers is essential in electronic commerce (EC) since it paves the way for mutual benefit. Users can research a business's reputation before deciding to do business with them. There are some aspects of the reputation system that are unflattering to both vendors and clients. For instance, as a consequence of their dishonesty or lack of effort while posting reviews, some users may create phantom feedback, which seems as fabricated assessments meant to boost their reputation.The proposed approach incorporates stage preprocessing, feature extraction, and model training. In order to prepare the data for the SA job, the pre-processing phase incorporates initial procedures that aid in data transformation. One way to improve the model's accuracy is to use features extraction to remove irrelevant data points. We kept using the HGAN model throughout training. This state-of-the-art method outperforms SVM and LSTM with an average accuracy of 91.26%.},
  keywords={Training;Support vector machines;Accuracy;Reviews;Phantoms;Feature extraction;Data models;Electronic commerce;Long short term memory;Business;Hierarchical Graph Attention Network (HGAN);Fake Review Detection;Term Frequency-Reverse Document Frequency (TF-RDF)},
