  author={Tian, Canwei and Hu, Haiyang and Li, Zhongji},
  booktitle={2022 International Conference on Industrial IoT, Big Data and Supply Chain (IIoTBDSC)}, 
  title={Graph Alignment Transformer for More Grounded Image Captioning}, 
  year={2022},
  volume={},
  number={},
  pages={95-102},
  abstract={The Industrial Internet of Things (IIoT) generates massive amounts of data that are the cornerstone for companies to increase productivity and provide reliable services. Based on these data, predictive and in-depth analysis can be used to identify weaknesses and make improvements. And how to analyze these data efficiently, effectively and safely requires us to explore. We expect to exploit these data by using methods of deep learning, and image captioning is one of the meaningful tasks. Image captioning aims to describe a given image in natural language. It is well believed that mining relationships between objects is a proven method to improve the performance of reasoning. These methods often extract the general relational expressions on another visual relationship benchmark. And it usually brings redundant connections between region pairs. In this paper, we propose a novel Graph Alignment Transformer (GAT) that models visual relationships in an unsupervised way to perform multimodal representation. Without taking the pre-training approach to obtain the explicit relational expressions, our model still achieves comparable results. Furthermore, we design a Graph Alignment (GA) module to explore semantic and visual alignment at node-level and graph-level, lead to accurate captions. We measured our method on the benchmark MSCOCO image captioning dataset and conduct ablation studies to investigate its effectiveness both quantitatively and qualitatively. Compared to state-of-the-art manners, our propose approach yields an impressive result.},
  keywords={Productivity;Visualization;Semantics;Supply chains;Natural languages;Benchmark testing;Transformers;Image Captioning;Visual Relationship;Textual Dependency;Graph Alignment;Grounded Caption},
  doi={10.1109/IIoTBDSC57192.2022.00028},
  ISSN={},
  month={Sep.},}@INPROCEEDINGS{10779915,
  author={Wei, Xiang and Zhou, Yang and Bai, Yaxin and Li, Zihui and Hu, Mingqi and Liu, Qiuming},
  booktitle={2024 IEEE 5th International Conference on Pattern Recognition and Machine Learning (PRML)}, 
  title={Short-Term Power Load Forecasting Based on CNN-BiLSTM-Attention Neural Network with ISSA Optimized}, 
  year={2024},
  volume={},
  number={},
  pages={63-69},
  abstract={Accurate short-term electricity load forecasting is essential to ensure the safe, reliable, and economically efficient operation of power systems. However, traditional forecasting methods are increasingly inadequate to meet the demand for ever more precise load predictions. In response to this issue, a CNN-BILSTM-Attention neural network method optimized based on an improved Sparrow Search Algorithm is proposed for short-term electricity load forecasting. Firstly, the Sin chaotic mapping strategy is utilized to initialize the Sparrow population, enhancing the diversity of population positions. Additionally, the cosine optimization algorithm is introduced into the formula for updating the joiner's position. This aims to address the decrease in global search capability in the later iterations of the Sparrow algorithm, thereby enhancing the accuracy of the Sparrow search algorithm; Next, the ISSA algorithm is employed to optimize the CNN-BILSTM-Attention neural network. This helps avoid overly random initialization of the CNN-BILSTM-Attention neural network's initial weight thresholds, thus enhancing the stability of prediction results; Finally, load forecasting is conducted and compared with other algorithmic models for analysis. Additionally, for the WOA-CNN-BILSTM-Attention model, RMSE decreases by 1.832%, with a corresponding MAE reduction of 1.073%. Similarly, for the PSO-CNN-BILSTM-Attention model, RMSE decreases by 1.804% and MAE by 1.026%. The results of the case study demonstrate that the proposed model can effectively enhance prediction accuracy and is valid for short-term load forecasting. By accurately predicting the power demand, the reasonable distribution and optimal dispatch of power can be realized, and the power grid fluctuations caused by the load mutation can be reduced, so as to ensure the quality of power supply.},
  keywords={Machine learning algorithms;Accuracy;Load forecasting;Biological system modeling;Neural networks;Power system stability;Prediction algorithms;Optimization;Thermal stability;Load modeling;Load Forecasting;Sparrow Search Algorithm;ISSA;CNN-BILSTM-Attention},
  doi={10.1109/PRML62565.2024.10779915},
  ISSN={},
  month={July},}@ARTICLE{10735154,
  author={Chang, Chuan-Yu and Khanum, Abida and Su, Sheng-Gui and Lebaku Moses, Manova and Liu, Kai-Xiang},
