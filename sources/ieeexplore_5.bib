  month={Nov},}@INPROCEEDINGS{10378263,
  author={Zhang, Yifei and Gu, Siyi and Gao, Yuyang and Pan, Bo and Yang, Xiaofeng and Zhao, Liang},
  booktitle={2023 IEEE/CVF International Conference on Computer Vision (ICCV)}, 
  title={MAGI: Multi-Annotated Explanation-Guided Learning}, 
  year={2023},
