  booktitle={2023 5th International Conference on Machine Learning, Big Data and Business Intelligence (MLBDBI)}, 
  title={An ensemble predictive model for vascular cognitive impairment using genetic algorithm}, 
  year={2023},
  volume={},
  number={},
  pages={226-230},
  abstract={Accuracy and explainability are critical for predicting the risk of VCI. While recent advances in machine learning methods have garnered attention, they face two persistent challenges when applied to predicting certain diseases: (1) The sparsity of case data, resulting in low accuracy; (2) The lack of explainability limits the scope of application. To solve these problems, we propose a novel ensemble prediction model based on genetic algorithm. Specifically, we screen out three high-accuracy models and key feature subsets through genetic algorithm, then fuse the three models through integration algorithm and genetic algorithm, and finally make predictions through voting mechanism. Our model improves by 20.1% compared to the baselines and explains the key features.},
  keywords={Machine learning algorithms;Fuses;Predictive models;Prediction algorithms;Feature extraction;Ensemble learning;Business intelligence;Vascular Cognitive Impairment;Feature Selection;Genetic Algorithm;Explainability},
  doi={10.1109/MLBDBI60823.2023.10481843},
  ISSN={2994-2977},
  month={Dec},}@INPROCEEDINGS{11043119,
  author={Zhou, Yun and Chen, Gang and Xue, Bing and Zhang, Mengjie and Rooney, Jeremy S. and Gordon, Keith C. and Killeen, Daniel P.},
  booktitle={2025 IEEE Congress on Evolutionary Computation (CEC)}, 
  title={Genetic Programming for High-Level Multi-Spectral Data Fusion in Fish Biochemical Analysis}, 
  year={2025},
  volume={},
  number={},
  pages={1-8},
  abstract={Accurate assessment of biochemical compositions in fish products is essential for quality control in the seafood industry and nutritional research. While spectroscopic techniques enable non-destructive analysis, each method has limitations in prediction accuracy and reliability. Multi-modal data fusion offers a promising solution, but developing robust fusion strategies remains challenging due to complex relationships between spectral features and biochemical properties. This paper presents GP-Fusion, a genetic programming-based high-level fusion method that integrates multiple spectroscopic modalities. Unlike conventional approaches, GPFusion evolves interpretable fusion functions to optimize predictions from diverse spectroscopy work-flows. A key innovation is the replicate variance penalty, which enhances prediction consistency across replicate measurements by capturing within-sample variability and mitigating batch effects. Experimental evaluations on three biochemical targets, including Omega-3, Omega-6, and monounsaturated fatty acids, show that GPFusion improves the coefficient of determination by 6.9%, 8.0%, and 2.6%, respectively. Compared with other high-level fusion strategies, GPFusion delivers more stable predictions with lower variance while maintaining competitive accuracy. Additional empirical studies confirms the effectiveness of the replicate variance penalty and reveal critical trade-offs between tree depth and terminal flexibility for evolving compact and interpretable fusion functions.},
  keywords={Industries;Spectroscopy;Technological innovation;Accuracy;Stacking;Data integration;Genetic programming;Predictive models;Fish;Optimization;spectroscopy;machine learning;fish biochemical composition analysis;data fusion;genetic programming},
  doi={10.1109/CEC65147.2025.11043119},
