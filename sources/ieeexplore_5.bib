  author={Li, Jiezhen},
  booktitle={2022 5th International Conference on Artificial Intelligence and Big Data (ICAIBD)}, 
  title={A Feature Engineering Approach for Tree-based Machine Learning Sales Forecast, Optimized by a Genetic Algorithm Based Sales Feature Framework}, 
  year={2022},
  volume={},
  number={},
  pages={133-139},
  abstract={In the new retail era, the sales pace and consumers’ consuming habits are disrupted dramatically by e-commerce and mobile consuming market developments and become various, volatile, and complicated. In sales forecasting, tree-based machine learning algorithms are popular and impressive. Feature engineering, on the other hand, is the most significant impediment to blending business and technological knowledge. An innovative way to solving the feature engineering problem of employing tree-based algorithms in sales forecast is proposed in this work. And a genetic algorithm is presented to enlarge the feature base and to explore potential values from features. To ensure that features with important business characteristics are picked, a novel feature framework for sales forecast is offered. Also given is a genetic algorithm for expanding the feature base and exploring potential values from features. The performance of tree-based models from AutoGluon will next be evaluated using a public dataset, Kaggle Rossmann sales data. The results suggest that the proposed strategy can improve the accuracy and stability of decision tree algorithms in sales forecasting significantly.},
  keywords={Machine learning algorithms;Systematics;Machine learning;Learning (artificial intelligence);Stability analysis;Forecasting;Thermal stability;sales forecast;feature engineering;genetic algorithm;tree-based algorithm;machine learning},
  doi={10.1109/ICAIBD55127.2022.9820532},
  ISSN={},
  month={May},}@ARTICLE{11054051,
  author={Ruíz-Guirola, David E. and López, Onel L. A. and Montejo-Sánchez, Samuel},
  journal={IEEE Open Journal of the Communications Society}, 
  title={Configuring Transmission Thresholds in IIoT Alarm Scenarios for Energy-Efficient Event Reporting}, 
  year={2025},
  volume={6},
  number={},
  pages={5490-5508},
  abstract={Industrial Internet of Things (IIoT) applications involve real-time monitoring, detection, and data analysis. However, the intermittent activity of IIoT devices and limited battery capacity pose critical challenges. This paper addresses these interconnected issues, focusing on extending the battery life of IIoT devices sensing events/alarms by minimizing the number of unnecessary transmissions. We propose a threshold-based transmission-decision policy based on the sensing quality and the network spatial deployment. We optimize the transmission thresholds using several approaches such as successive convex approximation, block coordinate descent methods, Voronoi diagrams, explainable machine learning, algorithms based on natural selection and social behavior, and Q-learning. Through numerical evaluation, we demonstrate significant performance enhancements in low-power IIoT environments, with Q-learning performing the best, while the block coordinate descending method performs the worst. We compare the proposed methods to a benchmark that assigns the same transmission threshold to all devices. In low-density scenarios, all proposed methods outperform the benchmark, while in high-density scenarios, only Voronoi-(i), K-nearest neighbors, and Q-learning show better performance. Power consumption is reduced by up to 95% in low-density scenarios compared to the benchmark and by 63% in high-density scenarios.},
  keywords={Industrial Internet of Things;Sensors;Energy efficiency;Correlation;Event detection;Power demand;Optimization;Benchmark testing;Q-learning;Performance evaluation;Alarm scenario;industrial Internet of Things;machine learning;spatial correlation;transmission threshold},
  doi={10.1109/OJCOMS.2025.3583867},
  ISSN={2644-125X},
  month={},}@INPROCEEDINGS{10485935,
  author={Gao, Xiaoya and Wu, Han and Chen, Haotian},
  booktitle={2024 IEEE 3rd International Conference on Electrical Engineering, Big Data and Algorithms (EEBDA)}, 
  title={An Optimized Genetic Algorithm-based BP Neural Network Model for the Classification of Human Pre-miRNA Subclasses}, 
  year={2024},
  volume={},
  number={},
  pages={1102-1109},
  abstract={Pre-miRNAs, as the precursor forms of miRNAs, possess significant research potential due to their intricate structural features. Accurate classification of pre-miRNAs is crucial for comprehending their roles in post-transcriptional gene regulation processes. This paper focuses on two distinct subclasses of pre-miRNAs: canonical miRNAs and mirtrons. Canonical miRNAs exhibit high conservation across species, whereas mirtrons display species-specific roles and expression patterns. To address the classification task, our study employed a Backward Propagation Neural Network (BPNN) model. While BPNN is renowned for its ability to learn and adapt from non-linear data, it often encounters challenges when dealing with high-dimensional spaces, leading to local optima. To further enhance the BPNN model, our study integrated BPNN with Genetic Algorithm (GA), a type of optimization algorithm inspired by the process of natural selection and genetics. GA is adept at exploring large and complex search spaces, which helps find alternative weight parameters for the BPNN model and thus enhances the model's convergence capability. The original dataset utilized in this study consists of human pre-miRNA sequences accompanied by accurately annotated class labels, sourced from miRBase (v21). A set of 25 features was derived from the original dataset, out of which 10 features were selected using SHapley Additive exPlanations (SHAP) technique as the independent variables. GA-BPNN's model performance was evaluated using independent testing datasets split from the pre-processed dataset and compared with other common Machine Learning (ML) techniques, demonstrating improved prediction performances for pre-miRNAs.},
  keywords={Backpropagation;Electric potential;Adaptation models;Neural networks;Predictive models;Prediction algorithms;Classification algorithms;Human Pre-miRNAs;Genetic Algorithm (GA);BP Neural Network (BPNN);Connection Weight},
  doi={10.1109/EEBDA60612.2024.10485935},
  ISSN={},
  month={Feb},}@ARTICLE{10731958,
  author={Zhao, Huimin and Wu, Yandong and Deng, Wu},
  journal={IEEE Transactions on Instrumentation and Measurement}, 
  title={Fuzzy Broad Neuroevolution Networks via Multiobjective Evolutionary Algorithms: Balancing Structural Simplification and Performance}, 
  year={2025},
  volume={74},
  number={},
  pages={1-10},
  abstract={Dynamic fuzzy broad learning system (DFBLS) is a fuzzy neural network based on the TSK fuzzy system and broad learning (BL). DFBLS possesses excellent model interpretability and efficient predictive performance. However, due to the phenomenon of rule explosion and the redundancy of network nodes, balancing network structure and performance has become a challenge in the construction of the DFBLS. Therefore, for the best balance between model predictive performance and network simplicity, a fuzzy broad neuroevolutionary network via multiobjective evolutionary algorithms (EAs) was developed in this article. First, an effective genetic encoding strategy was designed to represent the feature node building blocks and network connectivity relationships. The incremental mechanism and the randomly connected network relationships in the ILFR structure are replaced by the evolutionary framework. Second, a multiobjective optimization problem model is constructed with the optimization objectives of prediction performance and minimal network structure, and the corresponding objective functions are proposed. Finally, a self-adaptive mutation strategy with scaling genes is proposed for NSGA-II to optimize the accuracy and structure of the neural networks. The experiments demonstrate that SGNSGA-DFBLS achieves the best hypervolume (HV) values in 7 out of 9 public datasets. Its performance on the Air and pm2.5 datasets is either superior to or on par with other recently proposed models. SGNSGA-DFBLS can achieve high test accuracy with fewer fuzzy rules (FRs) and a compact network structure when constructing fuzzy broad network models.},
  keywords={Optimization;Accuracy;Predictive models;Neural networks;Evolutionary computation;Training;Learning systems;Linear programming;Feature extraction;Computational modeling;Complexity-accuracy trade-off;fuzzy broad networks;fuzzy rule (FR)-based classification systems;multiobjective evolutionary network framework},
  doi={10.1109/TIM.2024.3485438},
  ISSN={1557-9662},
  month={},}@INPROCEEDINGS{10393094,
  author={G, Bharathi Mohan and R, Prasanna Kumar and R, Elakkiya and G, Prasanna Kumar B},
  booktitle={2023 International Conference on Evolutionary Algorithms and Soft Computing Techniques (EASCT)}, 
  title={Medical Recommendations: Leveraging CRNN with Self-Attention Mechanism for Enhanced Systems}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={As a result of the exponential increase in the global population, many individuals have limited access to medical care. Robust medicine recommendation systems are needed to provide timely and accurate drug recommendations, improving patient outcomes and optimizing healthcare utilization. This research paper aims to propose a recommendation system for medical drugs that utilize sentiment analysis and captures main features using CRNN with a self-attention mechanism. This self-attention was instrumental in enabling the system to concentrate on critical segments of the text, while the CRNN architecture facilitated the capture of both spatial and temporal features of the input. The system yielded 96% accuracy, alongside precision, recall, and F1 scores of 95.4%, 93.8%, and 94.6%, surpassing multiple baseline models and hybrid models in recommending top drugs accurately for particular conditions. Hence, by providing efficient and effective drug recommendations, the proposed system can significantly benefit medical professionals and patients, particularly those in underserved communities.},
  keywords={Drugs;Sentiment analysis;Medical conditions;Correlation;Instruments;Sociology;Evolutionary computation;Medical Recommendations;Drug Reviews;Natural Language Processing;Self-Attention;CRNN;CNN;RNN},
  doi={10.1109/EASCT59475.2023.10393094},
  ISSN={},
  month={Oct},}@INPROCEEDINGS{10590519,
  author={Gireesan, Ganga and Pillai, Nisha and Rothrock, Michael J and Nanduri, Bindu and Chen, Zhiqian and Ramkumar, Mahalingam},
  booktitle={2023 International Conference on Computational Science and Computational Intelligence (CSCI)}, 
  title={Deep Sensitivity Analysis for Objective-Oriented Combinatorial Optimization}, 
  year={2023},
  volume={},
  number={},
  pages={519-525},
  abstract={Pathogen control is a critical aspect of modern poultry farming, providing important benefits for both public health and productivity. Effective poultry management measures to reduce pathogen levels in poultry flocks promote food safety by lowering risks of food-borne illnesses. They also support animal health and welfare by preventing infectious diseases that can rapidly spread and impact flock growth, egg production, and overall health. This study frames the search for optimal management practices that minimize the presence of multiple pathogens as a combinatorial optimization problem. Specifically, we model the various possible combinations of management settings as a solution space that can be efficiently explored to identify configurations that optimally reduce pathogen levels. This design incorporates a neural network feedback-based method that combines feature explanations with global sensitivity analysis to ensure combinatorial optimization in multiobjective settings. Our preliminary experiments have promising results when applied to two real-world agricultural datasets. While further validation is still needed, these early experimental findings demonstrate the potential of the model to derive targeted feature interactions that adaptively optimize pathogen control under varying real-world constraints.},
  keywords={Productivity;Pathogens;Sensitivity analysis;Scientific computing;Infectious diseases;Neural networks;Search problems;deep learning;combinatorial optimization;agriculture;pathogen;sensitivity analysis},
  doi={10.1109/CSCI62032.2023.00093},
  ISSN={2769-5654},
  month={Dec},}@INPROCEEDINGS{10612101,
  author={Heider, Michael and Krischan, Maximilian and Sraj, Roman and Hähner, Jörg},
  booktitle={2024 IEEE Congress on Evolutionary Computation (CEC)}, 
  title={Exploring Self-Adaptive Genetic Algorithms to Combine Compact Sets of Rules}, 
  year={2024},
  volume={},
  number={},
  pages={1-8},
  abstract={Rule-based machine learning (RBML) models are often presumed to be very beneficial for tasks where explainabil-ity of machine learning models is considered essential. However, their models are only really explainable as long as their rule sets are compact. This leads to the need for an optimizer to take prediction error and rule count as objectives. Given the highly complex fitness landscape of rule set learning tasks, good hyperparameters of the optimizer as well as their robustness against local minima is detrimental. In this paper, we explore the use of four self-adaptive genetic algorithms (SAGAs) for the optimization of a recent evolutionary RBML system to reduce the number of hyperparameters to tune and hopefully find better minima. To evaluate the advantages, we benchmark against a non-adaptive genetic algorithm (GA) on five real-world data sets. We find-with the support of a rigorous statistical analysis-that some of the SAGAs deliver a suitable alternative, which is easier to handle for non-experts in GA configurations. This is crucial for a wider application of this RBML method.},
  keywords={Runtime;Statistical analysis;Sociology;Machine learning;Benchmark testing;Robustness;Bayes methods;evolutionary machine learning;explainability;self-adaptation;learning classifier systems},
  doi={10.1109/CEC60901.2024.10612101},
  ISSN={},
  month={June},}@ARTICLE{9755976,
  author={Wang, Shigang},
  journal={IEEE Access}, 
  title={Learning Nonlinear Feature Mapping via Constrained Non-Convex Optimization for Unsupervised Salient Object Detection}, 
  year={2022},
  volume={10},
  number={},
  pages={40743-40752},
  abstract={Mimicking the biological visual attention mechanism to detect salient objects in images has been widely studied in recent years. Most of the existing computational models rely on external learning for saliency prediction, which however lack robustness in diversified scenes. In this paper, an unsupervised learning model is proposed to detect salient objects by fully exploiting the internal information of the scene. Specifically, we formulate saliency detection as a mathematical programming problem with which to learn a nonlinear feature mapping from multi-view features to saliency scores. The optimization objective is to maximize the between-class variance of the attended and background regions in the resulting saliency maps, which is statistically optimal. Moreover, to solve the non-convex constrained mathematical programming problem, a hybrid external point method based particle swarm optimization algorithm is developed to find the optimal solution in a rapid manner. Finally, extensive experiments are conducted on four classical saliency benchmark datasets to test the effectiveness of the proposed method and it shows superior qualitative and quantitative performance than the other 16 state-of-the-art unsupervised saliency models.},
  keywords={Feature extraction;Optimization;Biological system modeling;Saliency detection;Object detection;Visualization;Computational modeling;Mathematical programming;external point method;particle swarm optimization (PSO);salient object detection;unsupervised learning},
  doi={10.1109/ACCESS.2022.3166986},
  ISSN={2169-3536},
  month={},}@INPROCEEDINGS{10598640,
  author={Gao, Hongyu and Zhang, Yiwen and Hao, Fenghua and Song, Xueyan and Cai, Jinrui},
  booktitle={2024 39th Youth Academic Annual Conference of Chinese Association of Automation (YAC)}, 
  title={An Integrated Model-Driven and Data-Driven Method Based on CFD-PSO-RBF: Applications to Leakage Detection of Natural Gas Pipelines}, 
  year={2024},
  volume={},
  number={},
  pages={458-464},
  abstract={To address the issues of low accuracy, poor interpretability, and over-reliance on data in conventional model-based and data-based methods for pipeline leakage detection (PLD), an integrated model-driven and data-driven approach is proposed in this paper. In the presented method, the particle swarm optimised radial basis function (PSO-RBF) neural network is introduced into the computational fluid dynamics (CFD) model of the pipeline. Such the hybrid model improves the accuracy of the pipeline description by employing PSO-RBF to compensate for the errors generated by the model-based approach due to inaccurate meshing and imprecise modeling. The CFD model for the natural gas pipeline is initially built using the Ansys Fluent platform for offline training. It is then integrated with the PSO-RBF model. In the online detection stage, the trained CFD-PSO-RBF model is utilized for online prediction and real-time monitoring of pipeline operation. To demonstrate the application potential of the developed method, it is successfully employed to detect natural gas leakage in a laboratory simulating a real natural gas pipeline. Experimental results show that, in comparison to the traditional methods, the CFD-PSO-RBF method improves the accuracy of leakage detection and performs well in terms of response time.},
  keywords={Training;Accuracy;Computational modeling;Computational fluid dynamics;Pipelines;Data models;Real-time systems;Computational fluid dynamics (CFD);particle swarm optimization of radial basis function (PSO-RBF);Ansys Fluent platform;model-driven and data-driven;leakage detection},
  doi={10.1109/YAC63405.2024.10598640},
  ISSN={2837-8601},
  month={June},}@INPROCEEDINGS{10813889,
  author={Li, Yong and Jiang, Xiaoyang and Zhang, Kuang and Yang, Guohui},
  booktitle={2024 IEEE 10th International Symposium on Microwave, Antenna, Propagation and EMC Technologies for Wireless Communications (MAPE)}, 
  title={CNN-Genetic Algorithm Hybrid Approach for Rapid Inverse Design of Ultra-Wideband Metasurfaces}, 
  year={2024},
  volume={},
  number={},
  pages={1-3},
  abstract={In recent years, metasurfaces have demonstrated immense potential in electromagnetic wave manipulation and antenna design. Traditional metasurface design methods heavily rely on complex numerical calculations and optimization algorithms, resulting in high computational costs and lengthy design cycles. This paper presents a novel inverse design approach combining Convolutional Neural Networks (CNN) and Genetic Algorithm for metasurfaces. This method leverages CNN's advantages in feature extraction while utilizing genetic algorithm's global optimization capabilities to achieve rapid and precise design of metasurface units. Experimental results show that this approach can reduce design time from hours to minutes compared to traditional methods while maintaining high design accuracy.},
  keywords={Microwave antennas;Wireless communication;Design methodology;Microwave communication;Metasurfaces;Ultra wideband antennas;Convolutional neural networks;Optimization;Genetic algorithms;Microwave propagation;Metasurface Design;Convolutional Neural Networks;Genetic Algorithm;Deep Learning;Electromagnetic Wave Manipulation},
  doi={10.1109/MAPE62875.2024.10813889},
  ISSN={2995-1984},
  month={Nov},}@INPROCEEDINGS{10490799,
  author={Zhang, Qing and Luo, YingMing and Gong, Guoliang and Lin, Lin},
  booktitle={2023 International Conference on Sensing, Measurement & Data Analytics in the era of Artificial Intelligence (ICSMD)}, 
  title={An Evolutionary Algorithm-Based Neural Architecture Search for Feature Separation in Classification Problems}, 
  year={2023},
  volume={},
  number={},
  pages={1-6},
  abstract={In this paper, we propose Evolutionary Algorithm-based Neural Architecture Search(EvoNAS), which use Evolutionary Algorithm to find the optimized Convolutional Neural Network(CNN) and combine with Distribution Estimation to improve efficiency during the search process to improve the classification performance. The optimized CNN reveal the process of classification. It can classify the features according to specified classes, improving the classification accuracy of similar features while achieving the desired classification accuracy. We conduct feature separation experiments On the publicly available CIFAR dataset, we use EvoNAS to conducte feature separation experimentson. EvoNAS is subjected to feature separation experiments on the publicly available CIFAR dataset. The classification accuracy has been improved by 0.55% and 0.56% respectively compare to the original network. In addition, the feature visualization and attribution visualization improve the classificatory interpretability of CNN.},
  keywords={Data analysis;Estimation;Data visualization;Evolutionary computation;Search problems;Classification algorithms;Convolutional neural networks;convolutional neural network;neural architecture search;evolutionary algorithm;feature separation},
  doi={10.1109/ICSMD60522.2023.10490799},
  ISSN={},
  month={Nov},}@INPROCEEDINGS{10031202,
  author={Syu, Yang and Wang, Chien-Min},
  booktitle={2022 IEEE International Conference on Data Mining Workshops (ICDMW)}, 
  title={Using Genetic Programming to Identify Probability Distribution behind Data: A Preliminary Trial}, 
  year={2022},
  volume={},
  number={},
  pages={368-377},
  abstract={Before conducting any further applications or performing more advanced processing, analyzing and realizing the probability distribution of data is a crucial task. Traditionally, statistical methods are being developed for this procedure. In recent years, researchers in computer science have proposed and applied different machine learning-based techniques to address the abovementioned problem. However, the existing solutions remain problematic and inconvenient, such as the need for human intervention and the complexity of the resulting models. Thus, in this paper, without causing deficiency and inconvenience, a genetic programming-based approach for the identification of probability functions is proposed, implemented, and tested. Based on our empirical trials, in an immense search space of mathematical expressions, the proposed and developed approach can effectively recognize (retrieve) the probability distribution function behind data.},
  keywords={Computer science;Statistical analysis;Conferences;Computational modeling;Genetic programming;Probability distribution;Mathematical models;genetic programming;probability distribution;machine learning;explainable AI},
  doi={10.1109/ICDMW58026.2022.00056},
  ISSN={2375-9259},
  month={Nov},}@INPROCEEDINGS{10450920,
  author={Li, Jie and Yi, Rongxue and Wang, Bo and Guo, Xiang},
  booktitle={2023 China Automation Congress (CAC)}, 
  title={An Improved Adaptive PSO-LSTM Algorithm for the Gas Leakage Source Localization}, 
  year={2023},
  volume={},
  number={},
  pages={5537-5542},
  abstract={Gas leakage source localization is a matter of utmost security concern. However, the conventional mechanism model fails to accurately depict the actual diffusion environment during a leak, leading to diminished accuracy and substantial deviations in source estimation. To overcome this limitation and bolster the precision and reliability of gas leakage source localization, we introduce an improved adaptive PSO-LSTM algorithm. By leveraging field detection data obtained from ABB's Ability™ high-precision gas leakage detection system, we enhance the conventional particle swarm optimization algorithm (PSO) by fine-tuning learning factors and other critical parameters. Moreover, to harness the profound interpretability of the mechanism model and the potent learning capabilities of the LSTM neural network (data model), we substitute the fitness function of the traditional Gaussian plume diffusion model with training outcomes from the LSTM neural network, employing a sizable dataset. Empirical findings underscore that our refined algorithm attains superior accuracy and robustness in estimating source locations.},
  keywords={Training;Location awareness;Adaptation models;Neural networks;Estimation;Data models;Mathematical models;gas leakage source localization;adaptive PSO-LSTM;data model;mechanism model},
  doi={10.1109/CAC59555.2023.10450920},
